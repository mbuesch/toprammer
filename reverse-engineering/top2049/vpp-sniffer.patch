+			self.flushCommands()
+			time.sleep(0.1)
+			d = s.read()
+			d = ord(d[0]) | (ord(d[1]) << 8) | (ord(d[2]) << 16) | (ord(d[3]) << 24)
+			d ^= 0xFFFFFFFF
+			sys.stdout.write("0x%08X," % d)
+			count += 1
+			if count == 5:
+				count = 0
+				sys.stdout.write("\n")
+			else:
+				sys.stdout.write(" ")
+			sys.stdout.flush()
+
+		sys.exit(0)
 
 	def __bitfileUpload(self):
