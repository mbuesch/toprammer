+		self.cmdSetVPPVoltage(12)
+		self.queueCommand("\x0E\x28\x01\x00")
+		self.flushCommands()
+
+		import hc4094sniffer
+		s = hc4094sniffer.Sniffer("/dev/ttyUSB0", 4)
+		count = 0
+		for i in range(0, 0xFF+1):
+			self.cmdLoadVPPLayout(0)
